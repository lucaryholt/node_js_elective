<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>hadoc.xyz</title>
</head>
<body>

    <h1>Download file(s)</h1>

    <% files.forEach(function(file) { %>
        <p> * <%= file.name %></p>
        <p>
            ** <a href="http://<%= file.downloadLink %>" target="_blank"><%= file.downloadLink %></a>
        </p>
    <% }); %>

</body>
</html>